        <record id="ir_sequence_cf_credito_fiscal_fy" model="ir.sequence">
            <field eval="'Account Default Miscellaneous Journal '+time.strftime('%Y')" name="name" />
            <field name="code">cf.credito.fiscal</field>
            <field name="prefix">A0100100101/%(year)s/</field>
            <field name="padding">8</field>
        </record>
